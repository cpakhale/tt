apiVersion: v1
kind: Namespace
metadata:
  name: hacker-company
---
apiVersion: apps/v1
kind: Deployment
metadata:
  name: redis
  namespace: hacker-company
spec:
  replicas: 2
  selector:
    matchLabels:
      app: redis
  template:
    metadata:
      labels:
        app: redis
    spec:
      containers:
        - name: redis
          image: 134148934511.dkr.ecr.us-east-l.amazonaws.com/hr/redis:latest
          ports:
            - containerPort: 6379